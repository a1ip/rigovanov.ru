{
  "name": "MyBlog",
  "version": "0.0.1",
  "private": true,
  "description": "Resonal blog of Phil Rigovanov",
  "main": "index.js",
  "scripts": {
    "test": "write-good src/posts/**/*.md",
    "start": "nodemon -e md,styl,jade,css,html,js,coffee,json,cson --ignore build/ ./build.coffee",
    "serve": "serve build -p ${PORT:-3000}",
    "build": "coffee build.coffee",
    "deploy": "git push \"git@github.com:a1ip/rigovanov.ru.git\"; npm run build && cd build && git init . && git add . && git commit -m \"Deploy\"; git push \"git@github.com:a1ip/rigovanov.ru.git\" master:gh-pages --force && rm -rf .git"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a1ip/rigovanov.ru.git"
  },
  "author": {
    "name": "Phil Rigovanov",
    "email": "phil@rigovanov.ru",
    "url": "http://rigovanov.ru"
  },
  "license": "MIT",
  "devDependencies": {
    "coffee-script": "^1.9.2",
    "highlighter": "^0.0.3",
    "jade": "^1.9.2",
    "metalsmith": "^1.5.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-autoprefixer": "^1.0.1",
    "metalsmith-build-date": "^0.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-collections-paginate": "^2.0.1",
    "metalsmith-define": "^1.0.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-gist": "^0.3.0",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-pagination": "^1.0.1",
    "metalsmith-permalinks": "^0.4.0",
    "metalsmith-sitemap": "0.0.9",
    "metalsmith-snippet": "^1.0.1",
    "metalsmith-templates": "^0.7.0",
    "moment": "^2.10.0",
    "nodemon": "^1.3.7",
    "serve": "^1.4.0",
    "write-good": "^0.9.1"
  }
}
